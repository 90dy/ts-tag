{
  "fileTypes": [],
  "injectionSelector": "L:source.ts, L:source.tsx, L:source.js, L:source.jsx",
  "patterns": [
    {
      "include": "#scala-tagged-template"
    }
  ],
  "repository": {
    "typescript-comment": {
      "patterns": [
        {
          "match": "//.*$",
          "name": "comment.line.double-slash.ts"
        },
        {
          "begin": "/\\*",
          "end": "\\*/",
          "name": "comment.block.ts"
        }
      ]
    },
    "markdown-code-block": {
      "patterns": [
        {
          "match": ".*",
          "name": "markup.raw.code.markdown"
        }
      ]
    },
    "scala-tagged-template": {
      "begin": "(?<![_$[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))(?:(lang\\.)?(scala)|ext\\([\"']scala[\"']\\))\\s*(`)",
      "beginCaptures": {
        "1": {
          "name": "entity.name.function.ts"
        },
        "2": {
          "name": "entity.name.function.ts"
        },
        "3": {
          "name": "punctuation.definition.string.template.begin.ts"
        }
      },
      "end": "`",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.template.end.ts"
        }
      },
      "contentName": "meta.embedded.block.scala",
      "patterns": [
        {
          "include": "#typescript-comment"
        },
        {
          "include": "source.ts#template-substitution-element"
        },
        {
          "include": "source.scala"
        }
      ]
    }
  },
  "scopeName": "inline.scala.template"
}